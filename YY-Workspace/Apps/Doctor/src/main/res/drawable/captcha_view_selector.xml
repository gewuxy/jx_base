<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
<!--获取激活码的边框选择器-->
    <item android:state_pressed="false">
        <shape>
            <corners android:radius="@dimen/radius"/>
            <solid android:color="@color/text_ffffff"/>
            <stroke android:width="1dp" android:color="@color/text_0882e7"/>
        </shape>
    </item>

    <item android:state_pressed="true">
        <shape>
            <corners android:radius="@dimen/radius"/>
            <solid android:color="@color/text_ffffff"/>
            <stroke android:width="1dp" android:color="@color/text_e5e5e5"/>
        </shape>
    </item>

</selector>