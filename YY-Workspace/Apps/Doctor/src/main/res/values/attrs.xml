<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CircleProgressView">
        <!-- 进度的背景颜色 -->
        <attr name="circle_backProgress" format="reference|color"/>
        <!-- 进度条的颜色 -->
        <attr name="circle_progress" format="reference|color"/>
        <!-- 进度条的宽度 -->
        <attr name="circle_widthDp" format="dimension"/>
    </declare-styleable>

    <declare-styleable name="ModuleView">
        <!-- 文本 -->
        <attr name="module_text" format="string"/>
        <!-- 图标 -->
        <attr name="module_src" format="reference"/>
    </declare-styleable>

</resources>